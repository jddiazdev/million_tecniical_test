{
  "compilerOptions": {
    "target": "esnext",
    "module": "ESNext",
    "lib": ["esnext"],
    "jsx": "react-native",
    "allowJs": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "moduleResolution": "bundler",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules"],
  "extends": "@react-native/typescript-config/tsconfig.json"
}
